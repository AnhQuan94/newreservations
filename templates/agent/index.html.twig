{% extends 'base.html.twig' %}

{% block title %}Liste des agents{% endblock %}

{% block body %}

<h1>Liste des {{resource}}</h1>

<ul>
{% for agent in agents %}
    <li>{{ agent.name }} 
        <ul>
            {% for artist in agent.artists %}
            <li>{{ artist.firstname }}  {{ artist.lastname }}</li>
            {% endfor %}
        </ul>
    </li>
    
{% endfor %}


</ul>

{% endblock %}
